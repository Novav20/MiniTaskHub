{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/assets/*",
            "/api/*"
        ]
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    },
    "globalHeaders": {
        "/*": {
          "Cache-Control": "no-store, no-cache, must-revalidate, proxy-revalidate"
        }
    }
}